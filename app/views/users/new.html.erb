<% provide(:title, "Sign Up"); %>

<div class="info-page signup-page">

   <h1>Welcome to TabHub</h1>
   
   <%= form_for(@user) do |f| %>
      <%= render('users/fields', f: f); %>
      
      <br>
      
      <%= f.submit "Create Account", {class: "btn btn-primary create-account-btn"} %>
   <% end %>
   
</div>
